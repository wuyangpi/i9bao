.wraps {
  width 900px
  min-height 400px
  padding 5px 20px 10px
  border 1px solid #ccc
  margin-bottom 20px
  position relative
  overflow hidden
  .title {
    margin-left 5px
    font-size 16px
    font-weight 500
    position relative
    height 30px
    &:after {
      position absolute
      top 26px
      left 0
      display block
      content ''
      width 65px
      height 3px
      background #ff6e1b
    }
  }
  .ruleclass {
    width 95%
    position absolute
    bottom 20px
    left -10px
    .selectWidth {
      width 100%
    }
  }
  .marl20 {
    margin-left 20px
  }
  .marr20 {
    margin-right 20px
  }
  .mart20 {
    margin-top 20px
  }
  .marb20 {
    margin-bottom 20px
  }
}
  h3 {
    margin-bottom 20px
  }
